{%extends "base.html"%}


{% block login_form%} 
    <form id="loginForm" method="POST" >

        {{ form.csrf_token }}
        {{ form.username.label }}
        {{ form.username(size=20) }}
        {{ form.password.label }}
        {{ form.password(size=20) }}
        <input type="submit" value="Go">
    </form>

{% endblock %}
